{
  "name": "react-dymo",
  "version": "1.0.0",
  "description": "Collection of utilities for React library to handle Dymo Label Web Service",
  "author": "apenab",
  "license": "MIT",
  "repository": "apenab/react-dymo",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "dist/types.d.ts",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
